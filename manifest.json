{
  "name": "RESTClient",
  "version": "3.0.3",
  "manifest_version": 2,
  "description": "a debugger for RESTful web services.",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*/*",
    "downloads",
    "unlimitedStorage",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "RESTClient"
  }
}